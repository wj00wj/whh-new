/* pages/personlist/list.wxss */
.addBtn{
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background: #ff9800;
  text-align: center;
  border-radius: 8rpx;
  margin-top: 20rpx;
}
.btnbig{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 96rpx;
  background: #db9019;
  color: #fff;
  font-size: 36rpx;
  line-height: 96rpx;
  text-align: center;
}
.list{
  width: 90%;
  margin:20rpx auto 0;
  border:1px solid #dddd;
  border-radius: 5rpx;
  color: #777;
  font-size: 28rpx;
  line-height: 40rpx;
  padding: 20rpx;
  box-sizing: border-box
}
.imgs{
  float: left;
  width: 185rpx;
  height: 185rpx;
  margin: 15rpx;
  overflow:hidden;
}
.imgs image{
  width: 100%;
}